{
  "name": "masm_syntax_highlighter",
  "displayName": "MASM Syntax Highlighter",
  "description": "Basic syntax highlighter for the masm assembler language",
  "publisher": "Forrest Boyer",
  "version": "1.0.0",
  "icon": "./images/logo.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "masm",
      "extensions": [".asm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "masm",
      "scopeName": "source.masm",
      "path": "./syntaxes/masm.tmlanguage.json"
    }]
  }
}
